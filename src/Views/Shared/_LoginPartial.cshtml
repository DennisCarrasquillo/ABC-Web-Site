@{BLS_Inventory.Models.ActiveUser UserProps = (BLS_Inventory.Models.ActiveUser)Session["ActiveUser"];}

    <ul class="nav navbar-nav navbar-right">
    @if (UserProps != null)
    {
        <li class="nav-text" style="display: block">Welcome @UserProps.UserName</li>
        <li>@Html.ActionLink("Log Out", "LogOut", "Clients", routeValues: null, htmlAttributes: new { id = "loginLink" })</li>
    }
    else
    {
        <li>@Html.ActionLink("Sign Up", "RegisterView", "Clients")</li>     
        <li>@Html.ActionLink("Log in", "Login", "Clients", routeValues: null, htmlAttributes: new { id = "loginLink" })</li>
    }
    </ul>

